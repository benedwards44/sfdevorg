{ 
alert(&apos;Other Country is mandatory&apos;); 
} 
else 
{ 
var url=&quot;/apex/INFA_AV__verifyAddress?id=&quot;+&quot;{!Contact.Id}&quot;+&quot;&amp;type=&quot;+&quot;contact&quot;+&quot;&amp;mode=2&quot;; 

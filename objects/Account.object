	for(var i = 0; i &lt; AccountButtons.length; i++)
	{
		AccountButtons[i].value = &quot;Verifying...&quot;;
		AccountButtons[i].className = &quot;btnDisabled&quot;;
	}
}
var enableButtons = function()
{
	for(var i = 0; i &lt; AccountButtons.length; i++)
	{
		AccountButtons[i].value = &quot;Verify Addresses&quot;;
		AccountButtons[i].className = &quot;btn&quot;;
	}
}

disableButtons();

if({!ISNULL(Account.BillingStreet) &amp;&amp; ISNULL(Account.ShippingStreet)}) 
{
	alert(&quot;Please Enter an account address before verifying.&quot;);
	enableButtons();
}
else if({!((ISNULL(Account.BillingPostalCode) &amp;&amp; (ISNULL(Account.BillingCity) || ISNULL(Account.BillingState))) &amp;&amp; (ISNULL(Account.ShippingPostalCode) &amp;&amp; (ISNULL(Account.ShippingCity) || ISNULL(Account.ShippingState))))}) 
{
